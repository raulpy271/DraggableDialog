{
  "presets": ["@babel/env"],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-export-default-from",
    [
      "@babel/plugin-transform-modules-umd", {
        "exactGlobals" : true,
        globals: {
          index : 'Draggable'
        }
      }
    ]
  ]
}
